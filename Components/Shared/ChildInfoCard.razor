@namespace Denly.Components.Shared

<div class="compact-card child-info-card" @onclick="OnClick">
    <div class="card-title">Child Info</div>
    <div class="card-content chips-container">
        @ChildContent
    </div>
</div>

@code {
    [Parameter] public RenderFragment ChildContent { get; set; }
    [Parameter] public EventCallback OnClick { get; set; }
}
